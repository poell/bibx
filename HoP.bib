
@article{ WOS:000651727300001,
Author = {Batchen, Geoffrey},
Title = {Natural Relief: Antoine Claudet and the Stereoscopic Daguerreotype},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2020},
Volume = {44},
Number = {2-3},
Pages = {94-110},
Month = {JUL 2},
Abstract = {This article examines the stereo daguerreotypes produced by the
   England-based photographer Antoine Claudet, beginning in 1842 and
   continuing through the 1860s. An argument is made for the importance of
   these photographs, because of the technical innovations they represent
   but also due to their conceptual sophistication. In many of these works,
   Claudet turned looking at photographic images into a self-reflexive
   activity, into an activity overtly dependent on a productive interaction
   of eye and photograph.},
