@article{ WOS:000399653300006,
Author = {Athens, Elizabeth},
Title = {Relic, Photograph, Text: Picturing History in <i>Gardner's Photographic
   Sketch Book of the War</i>},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2017},
Volume = {41},
Number = {1},
Pages = {76-89},
Month = {FEB},
Abstract = {This article presents Gardner's Photographic Sketch Book of the War
   (1866) as Alexander Gardner's manifesto on the possibilities of the
   photograph. Although Walt Whitman famously maintained that the true
   story of the US Civil War could never be told, the Sketch Book suggests
   otherwise. Throughout the work, Gardner implicitly presented the
   photographic image as a form of relic, one that did not simultaneously
   destroy and disperse the site it portrayed. Its presentation of
   seemingly unmediated fact was intended to anchor the book's descriptions
   and imbue the writing with the imprimatur of truth. Presented as a
   natural emanation of war-rent sites, the photograph could allow for
   historical dissemination without decimation.},
