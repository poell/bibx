@article{ WOS:000402106900002,
Author = {Lerner, Jillian},
Title = {Nadar's Signatures: Caricature, Self-Portrait, Publicity},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2017},
Volume = {41},
Number = {2, SI},
Pages = {108-125},
Abstract = {As Nadar shifted his attention from caricature to photography in the
   1850s, he produced several fascinating hybrid images. Among these are
   experiments in photomontage and uncommon portraits that combine graphic,
   photographic, and autographic elements. The author shows how Nadar
   cultivated a number of recognisable signatures, which he reiterated and
   adapted for different image-types, products, and advertisements. Nadar's
   investment in self-portraiture, his penchant for publicity, and his
   ability to work across platforms and to balance artistic ambition and
   commercial enterprise are explored in depth.},
