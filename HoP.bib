@article{ WOS:000515574500004,
Author = {Toman, Jindrich},
Title = {From Carnival to Satire: Photomontage as a Commentary on Photography},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2019},
Volume = {43},
Number = {2, SI},
Pages = {144-155},
Month = {APR 3},
Abstract = {This article discusses two eras of manipulated photography -that of
   trick photography before 1914 and that of photomontage of the 1930s. The
   former had a carnivalesque character, while the latter highlighted
   political satire. Although profusely discussed in its days, trick
   photography was largely anonymous. By contrast, the 1930s brought about
