@article{ WOS:000422933000006,
Author = {Flint, Kate},
Title = {The Cultural History of the Flash Gun},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2017},
Volume = {41},
Number = {4, SI},
Pages = {395-411},
Abstract = {This article explores what it means to write a cultural history of the
   flash gun, the apparatus used to explode flash powder between the late
   nineteenth and the mid twentieth centuries. Finding safer, more reliable
   ways to ignite magnesium and its compounds was a priority for over forty
   years for those photographers who wished to light up darkness, whether
   their field of operation was deep underground or in their own living
   rooms. The article argues that by paying attention to a piece of
   equipment used to create artificial light, rather than to more customary
   topics of photographic history, we can see the interdependence of the
   many different materials that come together in creating a photographic
   image, as well as the intersection of technology, inventor, and
   practitioner. Moreover, this focus underscores that when it comes to the
   history of the technology surrounding photography, invention,
   adaptation, and improvement are happening simultaneously on many fronts,
   with a loose network of practitioners - amateur and professional
   photographers, and employees of photographic companies - working in both
   competition and collaboration in their attempts to tame the
   unpredictability of compound flash powder.},
