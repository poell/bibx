@article{ WOS:000246697900005,
Author = {Stokoe, Brian},
Title = {Class tourism and photography: The typological portraits of E. O. Hoppe
   and J. D. Beresford},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2007},
Volume = {31},
Number = {2},
Pages = {180-200},
Month = {SUM},
Abstract = {This paper examines the collaboration between the society portraitist E.
   O. Hoppe and the novelist J. D. Beresford that resulted in the
   publication of Taken from Life (1922), a book combining both
   photographic and literary typologies of the metropolitan lower classes.
   Here, Taken from Life is situated within its authors' respective careers
   and considered within the broader discourses of class differentiation
   and Social Darwinism active in Britain in the 1920s. The author argues
   that Taken from Life is permeated by a superficially benign, but
   ultimately paternalistic `class tourism', a form of voyeurism that
   derives from its uneasy combination of sociological speculation with
   both anecdote and fiction.},
