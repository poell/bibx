@article{ WOS:000355407300003,
Author = {Trnkova, Petra},
Title = {The Beginnings of Photography in Central Europe: Friedrich Franz and the
   First Daguerreotypes in Brno},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2015},
Volume = {39},
Number = {2},
Pages = {121-141},
Month = {APR 3},
Abstract = {This essay aims to examine the beginnings of photography in the Moravian
   capital of Brno, which, albeit for a short period between 1839 and 1842,
   was the gateway for the dissemination of photography to Moravia, Silesia
   and Bohemia. With the support of learned societies and other
   institutions in the region, local photographers were able to keep up
   with recent advances in daguerreotype processes. The most proficient
   among these enthusiasts was the Premonstratensian and Professor of
   Physics at the local Philosophy School, Friedrich Franz. Six
   daguerreotypes preserved in collections in Brno, as well as our
   knowledge of the social and institutional context of their origin,
   provide the foundation for this study of the beginnings of photography
   in Brno, and in Central Europe in general.},
Publisher = {TAYLOR \& FRANCIS LTD},
Address = {4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
Type = {Article},
