@article{ WOS:000231469500005,
Author = {Bailey, L and Thynne, L},
Title = {Beyond representation: Claude Cahun's monstrous mischief-making},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2005},
Volume = {29},
Number = {2},
Pages = {135-148},
Month = {SUM},
Abstract = {This article explores how Claude Cation (1894-1954) interrogated vision
   in her photographs as a means of control and categorization of the
   `other', particularly in relation to sexological and psychoanalytic
   discourses. In the sustained project of self-invention that Cahun
   undertook with her lifelong lover and stepsister, Marcel Moore
   (1892-1972), her key strategies are parody and the ironic reversal of
   dominant representations of gender, sexuality and race. These strategies
   are examined firstly in Cahun's mimicry of the portraits of paternal
   `others' and then through analysis of the strategic deployment, veiling
   and flaunting in her images of her Jewish and `third sex' identity, her
   real life performances, her writings and her objects.},
