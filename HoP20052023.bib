@article{ WOS:000256230700005,
Author = {Szymanowicz, Madej},
Title = {Antoni Wieczorek: A photographer at the turning point},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2008},
Volume = {32},
Number = {3},
Pages = {260-271},
Month = {FAL},
Abstract = {This article treats the art and writings of Antoni Wieczorek, one of the
   most influential figures in Polish photography of the 1930s. Whilst
   belonging to the pictorialist amateurs, Wieczorek criticised this very
   circle in numerous texts and argued, in contrast to the pictorialist
   credo, for photography as a utilitarian undertaking connected to current
   events. In his articles he underscored repeatedly the necessity of
   bringing photographic creativity to the mass market. The essential task
   of the photographer, he maintained, was the production of `artistic
   illustrations' for books and the press. Such views, articulated from the
   early 1930s, anticipate by several years the programme of Homeland
   Photography (fotografia ojczysta), consecrated by Jan Bulhak in 1937, in
   emulation of a German model, as a pictorial movement to further national
   propaganda. Homeland Photography set as its goal the documentation of
   Polish landscape, folklore, and architectural monuments, both past and
   present. As a result of the movement's documentary activities, in which
   Wieczorek played an important role, a new, idealized vision of the
   country emerged that promoted Poland's traditional values of nobility
   and folklore together with its nascent sites of heavy industry.},
