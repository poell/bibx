@article{ WOS:000244307600005,
Author = {Gaudreault, Andre},
Title = {One and many: Cinema as a series of series},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2007},
Volume = {31},
Number = {1},
Pages = {31-39},
Month = {SPR},
Note = {8th International Conference on Cinema Studies, Udine, ITALY, MAR 20-23,
   2001},
Abstract = {This article is concerned with the essential paradox of cinematography,
   a serial phenomenon that has as its goal the creation of an illusion of
   continuity and unity from diversity and multiplicity. It is concerned
   fundamentally with the dialectical movement in cinematography from one
   to many and from there to a new unity. The author argues that there
   existed a similar tension in early photography, which Sought initially
   to isolate a single `virtual' image of reality from a myriad of images.
   Joseph Nicephore Niepce's photograph showing the view from a window at
   his country estate of Le Gras was in reality a series of superimposed
   images, a palimpsest reflecting the passage of the time during the
   day-long exposure. The chronophotography of Etiemie-Jules Marey and of
   Eadweard Muybridge extends the single/multiple dialectic and leads to a
   discussion of Louis Lumiere's short film of 1895 La Sortie des usines
   Lumiere (Workers Leaving the Lumiere Factory) as a paradigm of
   cinematography's creation of unity from multiplicity. The discussion is
   extended finally to consider the construction of an early Lumiere
   programme at the Grand Cafe, Boulevard des Capucines, Paris, in December
   1898.},
Publisher = {TAYLOR \& FRANCIS LTD},
Address = {4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
