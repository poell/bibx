@article{ WOS:000238497600002,
Author = {Jacobs, S},
Title = {Amor vacui: Photography and the image of the empty city},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2006},
Volume = {30},
Number = {2},
Pages = {107-118},
Month = {SUM},
Abstract = {This article reviews the portrayal of the city in photography as
   deserted space, from Daguerre to the present day. The photographic
   rendering of the urban scene as architectonic and unoccupied originated
   with the early mechanical limitations of the medium. After photography
   became more sophisticated technically, photographers elected to depict
   the urban landscape as a `void' for symbolic, aesthetic and artistic
   purposes. The author places this photographic trope within the context
   of architecture and urban history, literary and artistic movements.},
