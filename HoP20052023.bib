@article{ WOS:000559563700001,
Author = {Hardy-Vallee, Michel},
Title = {The Photobook as Variant: Exhibiting, Projecting, and Publishing John
   Max's<i>Open Passport</i>},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2019},
Volume = {43},
Number = {4},
Pages = {399-421},
Month = {OCT 2},
Abstract = {In 1973, a Toronto magazine published the photographic bookOpen
   Passportby Montreal photographer John Max. A formally innovative
   narrative sequence about the dissolution of an artist's marriage, it has
   been a touchstone of Canadian photography since its publication.
   Simultaneously autobiographical, fictional, and experimental,Open
   Passportis a unique response to the representations of self and
   subjectivity in American photography of the 1970s, which championed the
   idea of photographers as authors and the book as privileged support for
   their images. Initially produced as an exhibition accompanied by a
   slideshow,Open Passportis a work that uses multiple supports for
   disseminating a photographic narration about the self. Using previously
   unavailable materials from John Max's archives, this article argues that
   these different supports should be considered as multiple variants of a
   single work, which upsets the hierarchical logic distinguishing original
   works from adaptations or derived works. This has important
   methodological consequences for the emerging field of photobook studies,
   which has given primacy to the history of books over that of
   exhibitions. The notion of performance is used to explain the
   relationship between the work and its supports, showing in turn the
   importance of the processes of editing and sequencing in the definition
   of the photographic work of art.},
Publisher = {TAYLOR \& FRANCIS LTD},
