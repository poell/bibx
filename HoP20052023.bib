@article{ WOS:000519725400001,
Author = {Locke, Nancy},
Title = {Charles Marville and the Landscapes of the Carrieres d'Amerique},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2019},
Volume = {43},
Number = {3},
Pages = {251-265},
Month = {JUL 3},
Abstract = {Charles Marville's photographs of Paris preserve the look of streets
   slated for demolition under Georges-Eugene Haussmann. This article
   examines his photographs of the Carrieres d'Amerique, or America
   Quarries - gypsum quarries in the outlying Parisian neighbourhood of
   Belleville. At a time when the rezoning of districts that were formerly
   extra muros was still controversial, Belleville was seen as a
   crime-ridden area. Marville's photographs become documents that refute
   contemporary narratives of criminality. Borrowing Walter Benjamin's view
   that the city `opens up' to the flaneur `as a landscape', the article
   analyses Marville's landscapes of the Carrieres d'Amerique as images
   that juxtapose the city, the work site, and the no-man's land or
   terrains vagues at Paris's outer limits.},
