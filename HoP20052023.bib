@article{ WOS:001289541700011,
Author = {Mickevicius, Emilia},
Title = {`Letting the pictures {[}…] do the job': Sandy Phillips at SFMOMA},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2023},
Volume = {47},
Number = {2},
Pages = {126-130},
Month = {APR 3},
Abstract = {Across three decades at the San Francisco Museum of Modern Art (SFMOMA),
   curator Sandra S. Phillips (American, born 1945) built a distinctive
   photography collection and exhibition programme driven by her
   institution's geographical setting. In response to Phillips's oral
   history excerpt for the project Framing the Field, this article
   identifies intuition as an emphasis of Phillips's interview and her
   curatorial practice. Challenging the perception of intuition as
   irreconcilable with traditional scholarly expertise, this article
   examines it as a form of knowledge and core component of ideation.
   Combined with her art historical acumen, Phillips's stance of
   receptivity towards SFMOMA's location in the Bay Area, California, the
   western USA and the greater Pacific Rim propelled her shaping of the
   institution's photography programme. Ultimately, her example indicates
   how intuition and expertise are mutually constitutive and intertwined.},
Publisher = {TAYLOR \& FRANCIS LTD},
Address = {2-4 PARK SQUARE, MILTON PARK, ABINGDON OR14 4RN, OXON, ENGLAND},
