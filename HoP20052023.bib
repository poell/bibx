@article{ WOS:000340378600006,
Author = {Sperlich, Tobias},
Title = {Photographing Mata'afa Iosefo: Encounters, Interactions and Engagements
   in Colonial Samoa},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2014},
Volume = {38},
Number = {3},
Pages = {296-314},
Month = {AUG},
Abstract = {During the decades around the turn of the twentieth century, tens of
   thousands of photographs were taken in Samoa by professional and amateur
   photographers. Because the production and consumption of photographs
   from this period are normally regarded as fundamentally western
   activities, these photographs are often mined exclusively for the
   information they yield about western attitudes and meanings. Yet, given
   that most of these photographs featured Samoans, they would have been
   intimately involved in the photographic process that created these
   images. Disregarding this aspect ignores the possibility that these
   photographs can also be expressive of Samoan socio-cultural values. This
   article analyses a selection of photographs of Mata'afa Iosefo, the high
   chief of Samoa, taken by a German scientist stationed in Samoa between
   1902 and 1905. It regards the photographic process as a space of
   cultural encounter and therefore reads these images as documents of the
   complex interplay of coloniser and colonised that characterised German
   Samoa.},
