@article{ WOS:000299933200006,
Author = {Ortiz-Echaguee, Javier and Montero Diaz, Julio},
Title = {Documentary Uses of Artistic Photography: <i>Spain. Types and
   Costumes</i> by Jose Ortiz Echague},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2011},
Volume = {35},
Number = {4},
Pages = {394-415},
Abstract = {The photographs of Jose Ortiz Echague are often read in terms of
   artistic and documentary values, with the two poles in tension within
   his work. Echague's project Spain. Types and Costumes was an attempt to
   document systematically traditional Spanish costumes. At the same time,
   Echague never abandoned pictorialist techniques, which are often
   considered contrary to documentary intentions. This article discusses
   the types and costumes series from a historic perspective based in the
   social uses of photography. This perspective shows how the same images
   were considered in different contexts as artistic or documentary images,
   or as propaganda icons in both sides during the Spanish Civil War.},
