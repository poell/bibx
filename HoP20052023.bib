@article{ WOS:000273295100004,
Author = {Barker, Geoff},
Title = {`Carte du Ciel': Sydney Observatory's Role in the International Project
   to Photograph the Heavens},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2009},
Volume = {33},
Number = {4},
Pages = {346-353},
Month = {NOV},
Abstract = {One of the most ambitious photographic projects the world has ever seen
   was announced at the 1887 International Astronomical Conference. Known
   as the Carte du Ciel' or `Mapping the Stars' project, its grand aim was
   to record on photographic plates the stars in the night sky. Worked on
   by eighteen observatories around the world, it was estimated that it
   would require 22,154 photographic plates. Although little known today,
   this project cemented the role of photography as a tool for astronomers
   and was in particular a major accomplishment for the Sydney Observatory,
   where it was worked on for over seventy years.},
