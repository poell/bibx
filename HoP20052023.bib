@article{ WOS:000340378600002,
Author = {Lerner, Jillian},
Title = {The Drowned Inventor: Bayard, Daguerre, and the Curious Attractions of
   Early Photography},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2014},
Volume = {38},
Number = {3},
Pages = {218-232},
Month = {AUG},
Abstract = {Hippolyte Bayard composed Le Noye (1840) as a performative fiction and a
   scandalous news item that highlights aspects of the competitive
   environment in which the French photographic pioneers vied for
   attention. This article examines diverse strategies of exposure,
   revelation, and narration employed by Bayard and L. J. M. Daguerre
   before 1840. The author shows how the marvellous products of early
   photography were positioned in relation to other sensational attractions
   in nineteenth- century Paris, such as the morgue, the diorama,
   journalism, and the promotional reclame.},
