@article{ WOS:000242598800004,
Author = {Melvin, Meg},
Title = {The gift of form: Brassai and the crypt of Saint-Sulpice},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2006},
Volume = {30},
Number = {4},
Pages = {359-371},
Month = {WIN},
Abstract = {This article provides a brief introduction to an intriguing series of
   Brassai photographs of sculpture stored in the church of Saint-Sulpice
   in Paris during World War 11. It employs a contemporary set of
   photographs on the same subject by an unidentified photographer of the
   French Press \& Information Service to furnish an illuminating
   comparison with the Brassai images. Placing these photographs in their
   historical context, the article explores the events surrounding the
   relocation of sculpture to the crypt of the church and highlights the
   extraordinary efforts made by the City of Paris to protect works of art
   during the war. It also discusses how Brassai and his contemporaries
   used the imagery of the wartime art evacuations as an effective and
   haunting metaphor for the human toll of war.},
