@article{ WOS:000286828600005,
Author = {Schroeter, Jens},
Title = {Technologies beyond the Still and the Moving Image: The Case of the
   Multiplex Hologram},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2011},
Volume = {35},
Number = {1},
Pages = {23-32},
Abstract = {Nothing seems more self-evident than the difference between still and
   moving images. But there are technologies that undermine this
   difference. One case is the multiplex hologram. After a technical
   introduction - which explains the technology and why its images are
   still and moving, depending on the relative movement between image and
   observer - three cultural occurrences of the multiplex hologram are
   discussed: one in the work of surrealist Salvador Dali, one in an essay
   of Umberto Eco on hyperreality and, finally, in the 1976 science-fiction
   movie Logan's Run. In each case the multiplex hologram occupies a place
   beyond `ordinary' reality. Based on these three case studies, I will
   argue that the construction of categorical oppositions like moving/still
   in relation to images is often connected to a naturalised and therefore
   problematic ontology. This will lead to the argument that the careful
   work of historiography must precede theoretical constructions.},
