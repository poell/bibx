@article{ WOS:000242598800001,
Author = {Macartney, Hilary},
Title = {William!Stirling and the talbotype volume of the `<i>Annals of the
   Artists of Spain</i>'},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2006},
Volume = {30},
Number = {4},
Pages = {291-308},
Month = {WIN},
Abstract = {As the first art book to be illustrated with photographs, William
   Stirling's Annals of the Artists of Spain (1848) pointed the way for the
   use of photographs and photographically illustrated books as essential
   tools for the study of art history. During preparation of his landmark
   art historical work, Stirling had demonstrated his interest in the
   reproduction of art by commissioning copies in oils and watercolours of
   Spanish Golden Age paintings. Many of these were then photographed for
   the Talbotype illustrations to the Annals. The Talbotypes had many
   shortcomings as reproductions of art, not least the fact that the
   finished volume included no direct reproductions of original paintings,
   though small-scale, original works of art including sculpture reliefs,
   drawings and prints were photographed directly. Stirling's decision to
   issue the Talbotypes as a separate, limited edition volume also reflects
   the many technical difficulties experienced during the project. The
   Talbotypes were taken by William Henry Fox Talbot's assistant, Nicolaas
   Henneman, whose bill for the supply of these to Stirling is published as
   an appendix to this article.},
