@article{ WOS:000468689600001,
Author = {Swensen, James},
Title = {A Strategy of Truth: Andreas Feininger and the Creation of Propaganda
   for the Office of War Information, 1942},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2019},
Volume = {43},
Number = {1},
Pages = {84-109},
Month = {JAN 2},
Abstract = {In 1942 photographer Andreas Feininger travelled across the United
   States documenting war industries for the Office of War Information, a
   New Deal agency. In the midst of world war, Feininger's straightforward
   photographs of plane production, copper mining, and other key industries
   were carefully crafted propaganda designed to combat German rhetoric and
   bolster public support and confidence in the war effort. Feininger's
   style, an admixture of modernist ideas and social detachment, found
   resonance within US ideological efforts that stressed data and facts,
   and became known as the `Strategy of Truth'.},
