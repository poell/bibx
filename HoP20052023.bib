@article{ WOS:000232599100002,
Author = {Thomas, A},
Title = {Witkiewicz, Trcka and Kesting: Pioneers in modernist portraiture},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2005},
Volume = {29},
Number = {3},
Pages = {217-227},
Month = {FAL},
Abstract = {The conventions of the photographed portrait have undergone significant
   changes within the period of its relatively short history. Some of these
   changes are linked to developments in painting, while others result from
   questions concerning the nature of the photographic medium. This essay
   examines the emergence in the early twentieth century of a new genre of
   portrait photography that reflects an increasing interest in human
   psychology and privileges the representation of emotional states over
   the depiction of faithful likeness. In addition to the three principal
   subjects of this essay, reference is made to painter-photographers of
   the late nineteenth century such as Edvard Munch, Alfons Mucha and
   August Strindberg, all of whom tested possibilities of the psychological
   portrait genre. Just how Witkiewicz, Trcka and Kesting broke the rules
   of conventional photographic portraiture is discussed by way of examples
   drawn from their photographic oeuvre.},
