@article{ WOS:000355407300005,
Author = {Fay, Brendan},
Title = {How to Hold a Camera: Harry Callahan's Early Abstractions},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2015},
Volume = {39},
Number = {2},
Pages = {160-176},
Month = {APR 3},
Abstract = {This article argues that body-camera relationships - issues of holding,
   mobility and display - informed the abstract qualities of some of Harry
   Callahan's early photographs from the 1940s. While these pictures have
   clear roots in earlier photographic precedents, they also engage
   problems related to those addressed by abstract painters of the same
   decade. The article positions Callahan's photographs as an independent
   contribution to this period's exploration of bodily experience and
   abstraction, but also suggests how camera-based photography from these
   years might complement and expand recent conversations about camera-less
   or process-based photographic abstraction.},
