@article{ WOS:000337565300005,
Author = {Grabczewska, Malgorzata Maria},
Title = {Adam and Katarzyna Potocki's Photograph Album of the Near East},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2014},
Volume = {38},
Number = {2},
Pages = {173-186},
Abstract = {This article examines a photograph album compiled by the Polish
   aristocrats Adam and Katarzyna Potocki of their journey to Egypt, the
   Holy Land and Istanbul in 1852 and 1853. The album, which is now in the
   National Library of Poland, consists of thirty-three calotype prints by
   three different photographers. The author discusses the album and its
   depiction of the Middle East in the first period of photography in the
   region. The article describes the processes by which the photographs
   were made, pointing out differences in the photographers' methods and
   the consequences of these differences for the aesthetic values of the
   photographs. It describes the working processes of each photographer and
   demonstrates three different methods of photographing the Middle East in
   the mid-nineteenth century.},
