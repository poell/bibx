@article{ WOS:000231469500004,
Author = {Hoving, KA},
Title = {Man Ray's disarming venuses: Deconstructing the classical torso in
   surrealist photography},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2005},
Volume = {29},
Number = {2},
Pages = {123-134},
Month = {SUM},
Abstract = {The female torso appears often in Surrealist photography. This article
   examines images by Man Ray and others in terms of their relation to
   classical form. It explores ways in which torsos appear as fragments, as
   body doubling corsets and mannequins, and as examples of the formless
   grotesque in Surrealist art and literature. It concludes by positioning
   the Surrealist appropriation of classical antiquity in the context of
   Surrealism's revolutionary exploration of the uncanny nature of meaning.},
