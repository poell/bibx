@article{ WOS:000268186400007,
Author = {Wakita, Mio},
Title = {Selling Japan: Kusakabe Kimbei's Image of Japanese Women},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2009},
Volume = {33},
Number = {2},
Pages = {209-223},
Month = {MAY},
Abstract = {This study analyses the representation of women in the souvenir
   photographs of the Japanese-run studio of Kusakabe Kimbei. Contrary to
   readings that stress the studio's production of exotic images of
   Japanese femininity, this study focuses oil the local signifying systems
   that informed his photographic practice and work. The aim is to analyse
   Kimbei's photographs of Japanese women within the broader context of
   late-nineteenth-century visual practices in Japan, particularly with
   regard to popular images of geisha and courtesans that were available in
   advertising and other media. The present study considers the staging,
   photographing and marketing of Kimbei's photographs of Japanese women,
   three aspects of the trade crucial to the visualisation of women in
   Japanese visual culture. Through a consideration of these stages, this
   study aims to promote an alternative reading of the photographer's
   Souvenir photographs of Japanese women.},
