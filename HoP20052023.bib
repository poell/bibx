@article{ WOS:000337565300002,
Author = {Smith, Graham},
Title = {Contemplating Photographs in Rauschenberg's Early Work},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2014},
Volume = {38},
Number = {2},
Pages = {113-136},
Abstract = {Robert Rauschenberg was a tireless maker of works of art, but he was
   also an insatiable consumer of photographic images, which he
   appropriated from contemporary newspapers and illustrated magazines.
   This study examines photographs from the print media that he
   incorporated into his combines in the 1950s and considers how those
   contribute to the design, fabrication and meaning of his work. A premise
   of the paper is that Odalisk, Rebus, Small Rebus and contemporaneous
   works repay sustained looking. In essence, it argues that Brian
   O'Doherty's notion of the `vernacular glance' circumscribed viewers'
   reception and analysis of Rauschenberg's work.},
