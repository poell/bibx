@article{ WOS:000231469500002,
Author = {Walker, I},
Title = {`Her eyes of fern':: The photographic portrait in `<i>Nadja</i>'},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2005},
Volume = {29},
Number = {2},
Pages = {100-113},
Month = {SUM},
Abstract = {Published in 1928, Andre Breton's Nadja is still an ambiguous,
   provocative book, not least for the way that text and photographs
   interrelate. There are several portraits in the book by two named
   photographers-Man Ray and Henri Manuel-and this paper initially examines
   the differing roles of their pictures. Next it turns to a glaring
   absence in the first edition: there is no photograph of Nadja herself.
   However, in the second edition, published in 1963, one was added: her
   eyes four times over. So, although there now exists an image of her, we
   may only see part of it. Nadja's apparent presence in the book is at the
   same time an absence, throwing into question Breton's own claims for its
   factuality.},
