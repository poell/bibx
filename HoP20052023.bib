@article{ WOS:000306222300002,
Author = {Gillespie, Sarah Kate},
Title = {John William Draper and the Reception of Early Scientific Photography},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2012},
Volume = {36},
Number = {3},
Pages = {241-254},
Abstract = {The essay explores the reception of early scientific photography through
   an examination of the work of John William Draper (1811-82). Throughout
   the 1840s, Draper - a chemist and trained medical doctor - used the
   daguerreotype as a tool to investigate various philosophical and
   scientific questions pertaining to the nature of light. While Draper
   sought both US and European audiences for his work, his focus on the
   process of the daguerreotype, rather than the results of that process,
   in addition to the unsettled terrain of the sciences and of the
   photograph itself during this period, caused many of his accomplishments
   to remain unknown by his contemporaries.},
