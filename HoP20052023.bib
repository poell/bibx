@article{ WOS:000236101700005,
Author = {Brückle, W},
Title = {On documentary style:: `anti-graphic photography' between the wars},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2006},
Volume = {30},
Number = {1},
Pages = {68-79},
Month = {SPR},
Abstract = {Olivier Lugon's Le Style documentaire. D'August Sander a Walker Evans,
   19201945, published in 2001, is a thought-provoking book on the
   `documentary style', a term once used by Walker Evans to distinguish his
   own artistic purpose from the propagandistic aims then associated with
   documentary photography. Lugon argues that Evans's works of the 1930s
   should be considered as testimonies of a style in its own right, and
   includes in his discussion the work of photographers such as August
   Sander, Albert Renger-Patzsch and Berenice Abbott. The present study
   discusses Lugon's meticulous argument. Objections can be made against
   the author's point that distinctions between the `New Objectivity' and
   the following `documentary style' are evident, and that Evans's own
   style agenda-often used as a means of characterising his oeuvre but
   fully developed as a category only by Lugon-does properly mirror his and
   his colleagues' practice during the thirties. It is argued here that the
   more one follows the path of style criticism, the more ambiguous
   historical concepts and facts become, and that acceptance of Evans's
   self-apology deflects attention from a long-established tradition of
   charging documentary pictures with aesthetic claims.},
