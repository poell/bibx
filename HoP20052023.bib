@article{ WOS:001289541700009,
Author = {Sims, Delphine},
Title = {Maria Morris Hambourg: A Curator Lighthouse for Photography},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2023},
Volume = {47},
Number = {2},
Pages = {111-117},
Month = {APR 3},
Abstract = {Founding curator of the Metropolitan Museum of Art's Department of
   Photographs, Maria Morris Hambourg's impact on the field of photography
   is incalculable. Across a multi-decade career, Hambourg influenced the
   canon of photography through largely monographic exhibitions. I use the
   terms searchlight and lighthouse as metaphors to describe her influence
   on curation and scholarship as well as the passion that focused her
   in-depth research. Hambourg's interest in the print process and
   photographic papers in tandem with historical context has been an
   important model for the generations of photography historians and
   curators that succeeded her. A committed expert on nineteenth-century
   France, Hambourg built relationships with photography collectors and
   conducted material and biographical research on artists such as Eug \&
   egrave;ne Atget, Nadar and Gustave Le Gray. Acknowledging her focus on
   European, white male photographers, I position Hambourg's exhibitions
   and writing as an important foundation for later research and
   scholarship that more thoroughly untangles histories of power, race and
   class, within the same canon she helped construct.},
