@article{ WOS:000652779300001,
Author = {Lopes, Shana},
Title = {Packaging Niagara:The Langenheim Brothers and the Transatlantic
   Circulation of Early Photography},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2020},
Volume = {44},
Number = {2-3},
Pages = {77-93},
Month = {JUL 2},
Abstract = {A few years after opening their celebrated photography studio in their
   new home in Philadelphia, German immigrants William and Frederick
   Langenheim produced eight almost identical daguerreotype panoramas of
   Niagara Falls. Keeping one set for themselves, they sent the others to
   multiple heads of state. The brothers' identities as American
   photographers and experiences as German immigrants, this article
   demonstrates, shaped their specific photographic vision of the
   impressive cataracts as well as its circulation abroad. By calling
   attention to the German dimensions of this project, this article
   underscores their panorama as an important articulation of
   German-American identity formation. The article thus highlights the
   intercultural nature of the Langenheim brothers' photographic practice,
   and the fact that the medium itself was rooted in international networks
   and trade in the mid-nineteenth century. Through a close examination of
   their advertising campaigns, their relationship with optician Peter
   Wilhelm Friedrich von Voigtlander and their extraordinary 1845 panorama,
   this article allows us to better understand how ideas of culture were
   negotiated through the making and distribution of early photography.},
