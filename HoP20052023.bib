@article{ WOS:000550084100001,
Author = {Gilmore, Dehn},
Title = {Large Direct Heads: Robert Crawshay's Dream of Life-Size Photography},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2019},
Volume = {43},
Number = {4},
Pages = {347-358},
Month = {OCT 2},
Abstract = {In the mid-1870s, Robert Crawshay started a now-forgotten contest
   designed to promote the development of direct life-size photography in
   Britain and the advent of a more objective photographic portrait. The
   contest saw much enthusiasm and many entries, but, as this article
   argues, ultimately ended up setting back the very causes it sought to
   advance. The competition shone a spotlight on problems with focus, tone,
   and especially scale when it came to the `life-size' direct portrait,
   and it managed to promote the role of subjectivity and the superiority
   of enlargement instead of its preferred ends.},
