@article{ WOS:000273295100007,
Author = {Rocco, Vanessa},
Title = {Pictorialism and Modernism at the Dresden <i>Internationale
   Photographische Ausstellung</i>},
Journal = {HISTORY OF PHOTOGRAPHY},
