@article{ WOS:000337565300003,
Author = {Kumar, Navjotika},
Title = {Repetition and Remembrance: The Rephotographic Survey Project},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2014},
Volume = {38},
Number = {2},
Pages = {137-160},
Abstract = {This essay considers the role of repetition and remembrance in the
   Rephotographic Survey Project (1977-79) and the related Third Views
   project (1997-2000), directed by the photographer Mark Klett. Premised
   on reconstructing topographically exact replicas of select photographs
   taken by various late-nineteenth-century survey photographers, both
   projects raise important questions about our relationship to this
   historic archive of photographs of Western America. By juxtaposing the
   original expedition photographs with `second views' taken a century
   later, and later with `third views' based on the `second views', these
   projects help to visualise changes in the original sites over the course
   of a hundred years, as well as a shorter span of twenty years. By thus
   pairing an original photograph with its second and even third views, and
   proposing the addition of subsequent views by future photographers,
   these rephotography projects, along with the most recent one called
   Reconstructing the View (2007-11), propose continually re-signifying
   viewers' relation to the Western American landscape and the past it
   represents. In doing so, they generate a construct of landscape that is
   textual and discursive or an effect of the relationship amongst shifting
   images and discourses. Such a construct, effectively manifest as well in
   the verbal/visual landscape representations of Robert Smithson, counters
   the traditional construct of landscape as a framed pictorial entity that
   can be viewed in a single glance. It also suggests that repetition
   functions in the Rephotographic Survey Project, Third Views project, and
   Reconstructing the View project, as a means of generating remembrance
   through the procedure that Sigmund Freud called `deferred action'.},
