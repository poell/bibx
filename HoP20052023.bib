@article{ WOS:000306222300004,
Author = {Godbey, Emily},
Title = {`Terrible Fascination': Civil War Stereographs of the Dead},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2012},
Volume = {36},
Number = {3},
Pages = {265-274},
Abstract = {This article proposes a relationship between the subject matter of the
   dead and the vehicle of stereography in American Civil War photography.
   In addition, it suggests a set of parallel oppositions between repulsion
   as compared with attraction and between Barthes's `reality effect' and a
   known illusion created by mechanical means. Civil War photographs
   commanded attention but repulsed with their horrific subject matter.
   Although stereography gave a convincing impression of three dimensions,
   viewers were aware of the mechanics generating this illusion. It appears
   that being aware of the nature of the optical device producing the
   illusion provided a buffer against the terrible nature of these images.},
