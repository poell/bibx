@article{ WOS:000252668000007,
Author = {Van Parys, Thomas},
Title = {A typology of the publicity still: Film for photograph},
Journal = {HISTORY OF PHOTOGRAPHY},
Year = {2008},
Volume = {32},
Number = {1},
Pages = {85-92},
Month = {SPR},
Abstract = {The release of a film is commonly supported by the distribution of film
   stills expressly made for publicity purposes. This paper focuses upon
   this under-researched phenomenon. In particular, it suggests a typology
   of the publicity photograph, based on three parameters: setting,
   appearance and gaze. The findings are illustrated by discussions of
   publicity stills from Alien and from Aliens. The article finishes with a
   reflection on the practice of publicity photography and points to
   tensions between mimesis and contrivance within the publicity still.},
Publisher = {TAYLOR \& FRANCIS LTD},
Address = {4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND},
